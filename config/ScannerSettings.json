{
    "ProxyServer":
    {
        "Host": "127.0.0.1",
        "DstPort": 443,
        "SrcPort": 129
    },
    "Logger":
    {
        "DirectoryPath": "full_path_to_directory",
        "Logfile": "file_name/console",
        "ErrorFile": "file_name/console",
        "ReportFile": "file_name/console",
        "LogLevel": 5,
        "VolumeSize": 50000,
        "VolumePrefix": "prefix",
        "LogfileExtension": ".log"
    },
    "Actions":
    {
        "ExitAfterError": true
    },
    "Network":
    {
        "MaximumSocketConnections": 100,
        "LocalPortsForBind": "129,560,1024-1500",
        "TimeoutBetweenRequests": 3,
        "TimeoutForAsyncWait": 3,
        "TimeoutForReceive": 5
    },
    "Engine":
    {
        "MaximumThreads": 8
    }
}