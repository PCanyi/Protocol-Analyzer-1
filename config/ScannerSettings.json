{
    "ProxyServer":
    {
        "Type": "socks4/socks5",
        "Host": "127.0.0.1",
        "DstPort": 443,
        "SrcPort": 129
    },
    "Logging":
    {
        "DirectoryPath": "full_path_to_directory",
        "LogFileName": "file_name/console",
        "ErrorFileName": "file_name/console",
        "Verbose": 6,
        "VolumeSize": 50000,
        "VolumePrefix": "prefix",
        "LogFileExtension": ".log"
    },
    "Network":
    {
        "MaximumSocketConnections": 100,
        "LocalPortsForBind": "129,560,1024-1500",
        "Timeouts":
        {
            "DelayBetweenRequests": 3,
            "ResponseAsyncWait": 10,
            "ConsiderAsError": true
        }
    },
    "Engine":
    {
        "ThreadsLimit": 8,
        "Events":
        {
            "ExitAfterError": true
        }
    },
    "Report":
    {
        "Format": "xml/json/pdf",
        "ReportFileName": "string"
    }
}